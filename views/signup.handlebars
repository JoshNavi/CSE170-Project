<!doctype html>

<html>
<head>
	<title>GO!outide</title>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href="/css/bootstrap.min.css" rel="stylesheet">
	<link href="/css/bootstrap-theme.min.css" rel="stylesheet">
	<link href="/css/gooutside.css" rel="stylesheet">
	<link href='https://fonts.googleapis.com/css?family=Titillium+Web:400,600italic,300' rel='stylesheet' type='text/css'>

</head>

<body>
	<div>

		<div class="navbar">
			<ul>
			  <a href="/">
					<li>
						<img class="back-btn" src="/images/back.svg" />
					</li>
				</a>
			</ul>
		</div>

		<div class="container">

			<form id="signupForm" role="form" method="get" action="/">
				<h3>Enter your info</h3>
				<div class="form-group">
					<label for="userName">User Name:</label>
					<input type="text" class="form-control" id="userName" placeholder="User Name" name="userName">
				</div>
				<div class="form-group">
					<label for="description">Password:</label>
					<input type="password" class="form-control" id="password" placeholder="Password" name="password">
				</div>
			<input type="submit" id="submitBtn" class="btn btn-default" value="Sign Up"></input>
		</form>

		</div>

	</div>

	<script src="https://code.jquery.com/jquery.js"></script>
	<script src="/js/bootstrap.min.js"></script>
	<script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
	<script src="/js/splash.js"></script>

</body>
</html>
